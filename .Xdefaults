!.Xdefaults

xman*topBox:              	 false
xman*bothShown:           	 true

!enable 96dpi fonts
Xft.dpi:                  	 96
Xft.antialias:            	 true

xpdf.viKeys:              	 true

!xosview settings
xosview*background:       	 black
xosview*foreground:       	 Orange

URxvt*depth:              	 24
!URxvt*geometry:          	 179x51
!URxvt*background:        	 Black
!URxvt*foreground:        	 White
!URxvt*background:        	 #1C1C1C
!URxvt*foreground:        	 #B4B4B4
URxvt*background:         	 #313031
URxvt*foreground:         	 White
URxvt*borderWidth:        	 1
URxvt*visualBell:         	 true
URxvt*inheritPixmap:      	 true
!URxvt*reverseVideo:      	 true
URxvt*insecure:           	 true
rxvt*tintColor:           	 #dfdfdf
rxvt*shading:             	 20!trans,0=opaque
rxvt*fading:              	 10!termunfocus
URxvt*font:               	 xft:terminus:size=14
URxvt*scrollBar:          	 true
URxvt*scrollBar_floating: 	 true
URxvt*scrollBar_right:    	 false
URxvt*scrollColor:        	 #000000
URxvt*saveLines:          	 2000
URxvt*termName:           	 rxvt-unicode
URxvt*cursorColor:        	 #d8d8d8
URxvt*colorBD:            	 red
URxvt*colorUL:            	 yellow
URxvt*urgentOnBell:       	 true
URxvt*scrollTtyOutput:    	 false
URxvt*scrollTtyKeypress:  	 true
URxvt*meta8:              	 true


!colors
!(unknown)
