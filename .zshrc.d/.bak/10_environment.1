
fpath=(~/.zsh/comp /Library/init/zsh/zshrc_local.d/site-functions /Library/init/zsh/zshrc.d/local-functions/opt /Library/init/zsh/zshrc.d/local-functions/darwin /Library/init/zsh/zshrc.d/local-functions/general /var/root/.comps /var/root/src/zshfunctions)

GREEN='\033[92m'
RED='\033[91m'
BLUE='\033[34m'
YELLOW='\033[33m'
ORANGE='\033[35m'
GREY='\033[90m'
CYAN='\033[36m'


RESET='\033[0m'
export GREEN RED BLUE YELLOW ORANGE GREY CYAN RESET

HISTSIZE=5000
SAVEHIST=100000
HISTFILE=~/.history
DIRSTACKSIZE=20


fignore=(.o .c~ .old .pro)


export EDITOR=/usr/local/bin/vim
# Unicode Locale
export LANG=en_US.UTF-8
export LC_ALL=en_US.UTF-8
export LC_CTYPE=en_US.UTF-8
export TERMINFO=$HOME/.terminfo

# Manpath & Manualpage search order
# export MANSECT=3:2:9:8:1:5:4:7:6:n


export PATH=/usr/bin:/usr/local/bin:/usr/local/texlive/2009/bin/universal-darwin:/opt/subversion/bin:/Developer/usr/bin:$PATH
export RHOME="/Library/Frameworks/R.framework/Versions/Current/Resources"
export MANPATH=$MANPATH:/usr/share/man:/usr/local/share/man:/opt/local/share/man
export XDG_CONFIG_HOME=/opt/local/etc/xdg 



# Syntax highlight for less with 'source-highlite'
PAGER='less -X -M'
# export LESSOPEN="| /usr/local/bin/src-hilite-lesspipe.sh %s"
# export LESS=' -R '

